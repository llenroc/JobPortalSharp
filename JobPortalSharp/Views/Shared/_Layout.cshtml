    <link rel="stylesheet" type="text/css" href="/content/master_a-master.css?446559478" />
    <link rel="stylesheet" type="text/css" href="/content/index.css?4154027239" id="pagesheet" />
    <link href="~/Content/style.min.css" rel="stylesheet" />
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/bootstrap-switch/bootstrap3/bootstrap-switch.min.css" rel="stylesheet" />
    @RenderSection("StyleSheets", false)
    <script type="text/javascript">
        document.documentElement.className = document.documentElement.className.replace(/\bnojs\b/g, 'js');
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?v=3.exp&sensor=false&libraries=places&key=AIzaSyCX_MP1Cua0YlkhoIjxtt3QOTq26F_eaQQ"></script>
    @RenderSection("HeaderScripts", false)
    <style type="text/css">
        a:link {
            text-decoration: none !important;
        }

        a.btn {
            color: #ffffff;
        }

        input.logout {
            background-color: transparent;
        }

        .pac-container:after {
            /* Disclaimer: not needed to show 'powered by Google' if also a Google Map is shown */
            background-image: none !important;
            height: 0;
            padding: 0;
            margin: 0;

        }
    </style>
</head>
<body>
    @if (User.Identity.IsAuthenticated)
    {
        <nav class="navbar navbar-fixed-top navbar-inverse navbar-right">
            <div class="container">
                @using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { style = "display: inline", @class = "navbar-right" }))
                {
                    <div class="form-group">
                        <input type="submit" value="Log Out" class="navbar-text logout" />
                    </div>
                }
                <span class="navbar-text navbar-right"> | </span>
                <p class="navbar-text navbar-right">Signed in as <a href="#" class="navbar-link">@User.Identity.Name</a></p>
            </div>

        </nav>
    }
    <nav class="navbar navbar-default">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="~/">MyJobPortal</a>
            </div>
            <div id="navbar" class="collapse navbar-collapse navbar-right">
                <ul class="nav navbar-nav">
                    <li><a href="~/">Home</a></li>
                    @if (User.Identity.IsAuthenticated == false)
                    {
                        <li><a href="~/account/employerlogin">Employer</a></li>
                        @*<li><a href="~/account/register">Register</a></li>*@
                        @*<li><a href="~/account/login">Login</a></li>*@
                    }
                    else if (User.IsInRole("Employer"))
                    {
                        <li><a href="~/jobposts">My Posts</a></li>
                    }
                </ul>
            </div>
        </div>
    </nav>

    <div class="clearfix borderbox" id="page">
        <div class="position_content" id="page_position_content">
            @RenderBody()
            <div class="verticalspacer"></div>
        </div>
    </div>
    <div class="preload_images">
        <img class="preload" src="~/images/u2395-r.png" alt="" />
        <img class="preload" src="~/images/u2365-r.png" alt="" />
        <img class="preload" src="~/images/u2377-r.png" alt="" />
        <img class="preload" src="~/images/u2389-r.png" alt="" />
    </div>
